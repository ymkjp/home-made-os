	$(MAKE) -C color     cleanall
	$(MAKE) -C csvv      cleanall
	$(MAKE) -C cvtg      cleanall
	$(MAKE) -C gview     cleanall
	$(MAKE) -C invader   cleanall
	$(MAKE) -C keyview   cleanall
	$(MAKE) -C kuha      cleanall
	$(MAKE) -C luckynum  cleanall
