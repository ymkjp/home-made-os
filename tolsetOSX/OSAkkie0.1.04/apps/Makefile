MAKE     = make -r

default :
	$(MAKE) -C 1line
	$(MAKE) -C bball
	$(MAKE) -C bonno
	$(MAKE) -C calc
	$(MAKE) -C clock
	$(MAKE) -C color
	$(MAKE) -C csvv
	$(MAKE) -C cvtg
	$(MAKE) -C gview
	$(MAKE) -C invader
	$(MAKE) -C keyview
	$(MAKE) -C kuha
	$(MAKE) -C luckynum
	$(MAKE) -C menu
	$(MAKE) -C mmlplay
	$(MAKE) -C mtorz
	$(MAKE) -C noodle
	$(MAKE) -C osakcnt
	$(MAKE) -C stbeep
	$(MAKE) -C tview
	$(MAKE) -C type
	$(MAKE) -C walk
	$(MAKE) -C winhello

clean :
	$(MAKE) -C 1line     clean
	$(MAKE) -C bball     clean
	$(MAKE) -C bonno     clean
	$(MAKE) -C calc      clean
	$(MAKE) -C clock     clean
	$(MAKE) -C color     clean
	$(MAKE) -C csvv      clean
	$(MAKE) -C cvtg      clean
	$(MAKE) -C gview     clean
	$(MAKE) -C invader   clean
	$(MAKE) -C keyview   clean
	$(MAKE) -C kuha      clean
	$(MAKE) -C luckynum  clean
	$(MAKE) -C menu      clean
	$(MAKE) -C mmlplay   clean
	$(MAKE) -C mtorz     clean
	$(MAKE) -C noodle    clean
	$(MAKE) -C osakcnt   clean
	$(MAKE) -C stbeep    clean
	$(MAKE) -C tview     clean
	$(MAKE) -C type      clean
	$(MAKE) -C walk      clean
	$(MAKE) -C winhello  clean

cleanall :
	$(MAKE) -C 1line     cleanall
	$(MAKE) -C bball     cleanall
	$(MAKE) -C bonno     cleanall
	$(MAKE) -C calc      cleanall
